<!-- Page Header Start -->
<div class="container-fluid page-header pt-5 mb-6 wow fadeIn" data-wow-delay="0.1s">
    <div class="container text-center pt-5">
        <div class="row justify-content-center">
            <div class="col-lg-7">
                <div class=" p-5">
                    <h1 class="text-white display-6 text-uppercase mb-3 animated slideInDown">Projelerimiz</h1>
                    <nav aria-label="breadcrumb animated slideInDown">
                        <hr />
                        <ol class="breadcrumb justify-content-center mb-0">

                            <li class="breadcrumb-item "><a class="text-primary" asp-controller="home" asp-action="ServiceTrading">Ticari Gemiler</a></li>
                            <li class="breadcrumb-item "><a class="text-primary" asp-controller="home" asp-action="ServiceMilitary">Askeri Gemiler</a></li>
